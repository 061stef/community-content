{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Page"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "slug": {
      "type": "uid",
      "required": false,
      "default": ""
    },
    "parentPage": {
      "model": "page"
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "component": "meta.metadata"
    },
    "contentSections": {
      "type": "dynamiczone",
      "components": [
        "sections.hero",
        "sections.bottom-actions",
        "sections.feature-columns-group",
        "sections.feature-rows-group",
        "sections.testimonials-group",
        "sections.large-video"
      ]
    }
  }
}
